{
  "name": "ondefoi",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --go",
    "lint": "npx expo lint",
    "format": "prettier --write .",
    "sentry:pre-update": "npx expo export --dump-sourcemap",
    "sentry:upload-sourcemaps": "npx sentry-expo-upload-sourcemaps dist",
    "app:prepare": "npm run format && npm run lint && npx expo install --fix && npx expo-doctor@latest",
    "app:build": "eas build --profile staging",
    "app:update": "npm run sentry:pre-update && eas update --channel staging --message \"Updating in Staging\" && npm run sentry:upload-sourcemaps",
    "app:build:production": "eas build --profile production",
    "app:update:production": "npm run sentry:pre-update && eas update --channel production --message \"Updating in Production\" && npm run sentry:upload-sourcemaps",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.6",
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.7",
    "@shopify/flash-list": "2.0.2",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "expo": "~54.0.25",
    "expo-application": "~7.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.9",
    "expo-navigation-bar": "~5.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.13",
    "firebase": "^12.6.0",
    "lucide-react-native": "^0.554.0",
    "mitt": "^3.0.1",
    "numeral": "^2.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.5.1",
    "yup": "^1.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@types/numeral": "^2.0.5",
    "@types/react": "~19.1.10",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
